	<Template
		ShowOnMenu="0"
		Name="Tornado for Windows &gt;= 3.0"
		Platforms='"Windows"'>
		<Config Name="Release">
			<Includes>
				<Include Dir="%(INCLUDE)"/>
			</Includes>
			<Menu>
				<Target
					Name="Compile"
					MenuCaption="&amp;Compile"
					SaveOption="SaveCurrent"
					RunFromDir="%rw">
					<Exec
						CmdLine="tornadomake -f %rpMakefile -C %rp %b default %n.o"
						Type="Slick-C"/>
				</Target>
				<Target
					Name="Build"
					MenuCaption="&amp;Build"
					SaveOption="SaveWorkspaceFiles"
					RunFromDir="%rw">
					<Exec
						CmdLine="tornadomake -f %rpMakefile -C %rp %b default"
						Type="Slick-C"/>
				</Target>
				<Target
					Name="Rebuild"
					MenuCaption="&amp;Rebuild"
					ShowOnMenu="HideIfNoCmdLine"
					SaveOption="SaveWorkspaceFiles"
					RunFromDir="%rw">
					<Exec
						CmdLine="tornadomake -f %rpMakefile -C %rp %b rclean"
						Type="Slick-C"/>
				</Target>
				<Target
					Name="Debug"
					MenuCaption="&amp;Debug"
					ShowOnMenu="HideIfNoCmdLine"
					RunFromDir="%rw">
					<Exec/>
				</Target>
				<Target
					Name="Execute"
					MenuCaption="E&amp;xecute"
					ShowOnMenu="HideIfNoCmdLine"
					RunFromDir="%rw">
					<Exec/>
				</Target>
			</Menu>
		</Config>
	</Template>
