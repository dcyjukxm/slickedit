<?xml version="1.0" ?>
<!DOCTYPE SETemplate SYSTEM "http://www.slickedit.com/dtd/vse/setemplate/1.0/setemplate.dtd">
<SETemplate Version="1.0" Type="Item">

  <TemplateDetails>
    <Name>Java JUnit Test Fixture</Name>
    <Description>JUnit fixture for running tests</Description>
    <SortOrder>0</SortOrder>
    <DefaultName>UnitTestName</DefaultName>
