checkout_read_only=ss get %f -o@%t -exitcode
remove=ss delete %f -o@%t -exitcode
history=ss history %f -o@%t -exitcode
difference=ss difference %f -o@%t -exitcode
properties=ss properties %f -o@%t -exitcode
manager=ssexp
lock=
archive_filespec=
styles=comment_file,error_file,sstree,sserror_file_rc,vcp_required
vcsproject=ssproject,ss cd "%s" -o%t -exitcode

[PVCS for DOS]
lock=vcs -y -l %f
remove=
history=vlog %f
difference=vdiff -r %f
properties=vlog -b %f
checkin_new=put -t@%c %f
checkout=get -l %f
checkin=put -m@%c -t@%c %f
checkin_discard=vcs -u %f
checkout_read_only=get %f
archive_filespec=*.??v
styles=comment_file,error_redir,dosrc,pvcs_wildcards

