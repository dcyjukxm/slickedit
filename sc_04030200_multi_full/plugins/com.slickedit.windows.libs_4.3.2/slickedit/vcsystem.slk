[Source Safe for Windows]
checkin_new=sswcl add %n%e -c@%c -o@%t -exitcode
checkout=sswcl checkout %f -o@%t -exitcode
checkin=sswcl update %f -c@%c -i- -o@%t -exitcode
checkin_discard=sswcl uncheckout %f -i- -o@%t -exitcode
checkout_read_only=sswcl get %f -o@%t -exitcode
remove=ss delete "%f" -o@%t -exitcode
history=ss history "%f" -o@%t -exitcode
difference=ss difference "%f" -o@%t -exitcode
properties=ss properties "%f" -o@%t -exitcode
lock=
archive_filespec=
archive_filespec=
styles=comment_file,error_file,sstree,sserror_file_rc,vcp_required
vcsproject=ssproject,sswcl cd %s -o%t -exitcode

[Source Safe for Windows >=4.0]
checkin_new=ss add "%n%e" -c@%c -o@%t -exitcode
