[GNU RCS]
checkin_new=ci -i -q "-m<%c" %n%e
checkout=co -q -l %n%e
checkin=ci -q "-m<%c" %n%e
checkout_read_only=co -q %n%e
checkin_discard=rcs -u -q %L %n%e
lock=rcs -l %f
remove=
history=rlog %f
difference=rcsdiff -r %f
properties=rlog -t %f
archive_filespec=RCS\*.*
styles=comment_file,dosrc,error_redir

