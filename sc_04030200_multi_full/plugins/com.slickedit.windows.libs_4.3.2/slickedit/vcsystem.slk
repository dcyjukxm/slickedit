[TLIB For OS/2]
checkin_new=os2tlib n %f @%c
checkin=os2tlib u %f @%c
checkout=os2tlib e %f
checkin_discard=os2tlib ud %f
checkout_read_only=os2tlib eb %f
archive_filespec=*.?$?
lock=
remove=
history=tlib l %f
difference=
properties=tlib t %f
manager=tlib
styles=comment_file,error_stdout
vcsproject=dir

